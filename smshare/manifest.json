{

  "name": "Smshare - SMS : Short Message Share",

  "version": "1.6",

  "description": "__MSG_manifestDescription__",
  
  "default_locale" : "en",
  
  "browser_action": {
    "default_icon": "images/iconT.png",
    "popup": "popup.html",
    "default_title" : "Smshare"
   },
   
  "icons": {
    "48" : "images/icon48T.png",
    "64" : "images/icon64T.png",
    "128": "images/icon128T.png"
   },
   
  "background_page": "background.html",
  
  "options_page": "options.html",
  
  "permissions": [
    "http://*/*",
    "tabs"
  ]

}
